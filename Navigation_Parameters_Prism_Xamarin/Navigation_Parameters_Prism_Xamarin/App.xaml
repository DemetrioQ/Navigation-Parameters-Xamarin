<?xml version="1.0" encoding="utf-8" ?>
<prism:PrismApplication  xmlns:prism="http://prismlibrary.com" xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Navigation_Parameters_Prism_Xamarin.App">
    <prism:PrismApplication.Resources>
        <ResourceDictionary>
            <Style TargetType="NavigationPage">
                <Setter Property="HasNavigationBar" Value="True" />
                <Setter Property="BarTextColor" Value="Black" />
                <Setter Property="BarBackgroundColor" Value="Teal" />
            </Style>
        </ResourceDictionary>
    </prism:PrismApplication.Resources>
</prism:PrismApplication>